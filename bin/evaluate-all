#!/usr/bin/env sh

set -e

ARGS="--directory ~/Code/link"

bin/mosaik evaluate --input candidates/structural-logical-contributor.csv --output evaluation/structural-logical-contributor.csv --statistics statistics/structural-logical-contributor.csv

